{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    // "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@config": ["src/config"],
