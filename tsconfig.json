{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
      "@components/*": ["src/components/*"],
      "@config": ["src/config"],
      "@containers/*": ["src/containers/*"],
      "@data/*": ["src/data/*"],
      "@layouts/*": ["src/layouts/*"],
      "@plugin/*": ["src/plugins/*"],
      "@scripts/*": ["src/scripts/*"],
      "@styles/*": ["src/styles/*"],
      "@util/*": ["src/utilities/*"]
    }
	}
}
